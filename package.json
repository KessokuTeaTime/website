{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prepare": "pnpm exec tsx scripts/prepare.ts",
    "dev": "pnpm run prepare && astro dev",
    "prebuild": "pnpm exec tsx scripts/prebuild.ts",
    "build": "pnpm run prebuild && astro build",
    "preview": "pnpm run prepare && astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.2",
    "@astrojs/vue": "^5.1.0",
    "@astropub/md": "^1.0.0",
    "@vueuse/core": "^13.6.0",
    "astro": "^5.12.6",
    "tsx": "^4.20.3",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "sass-embedded": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
